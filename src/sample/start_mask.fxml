<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import java.net.*?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="TOP_CENTER" hgap="10" vgap="10" styleClass="root">
    <Label id="brand" text="${'Test\nDriven\nDevelopment\nTrainer'}"
           GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <Button id="start" text="Start" onAction="#manageStart"
            GridPane.columnIndex="0" GridPane.rowIndex="10"/>
    <Label id="autor" text="by Amigos"
           GridPane.columnIndex="0" GridPane.rowIndex="30"/>
    <stylesheets>
        <URL value="@layout.css" />
    </stylesheets>

</GridPane>

